<p>10ページ</p>
<div class = 'container'>
 <h2 class = 'mt-5 text-center'><%= @customer.last_name %><%= @customer.first_name %>さんの会員情報編集</h2>
  <div class = 'row'>
   <%= form_with model: @customer, url:admin_customer_path do |f| %>

    <div class = 'form-group'>
     <%= f.label :会員ID %>
     <%= f.label @customer.id %>
    </div>

    <div class = 'form-group'>
     <%= f.label :氏名 %>
     <%= f.text_field :last_name %>
     <%= f.text_field :first_name %>
    </div>

    <div class = 'form-group'>
     <%= f.label :フリガナ %>
     <%= f.text_field :last_kana %>
     <%= f.text_field :first_kana %>
    </div>

    <div class = 'form-group'>
     <%= f.label :郵便番号 %>
     <%= f.text_filed :zip %>
    </div>

    <div class = 'form-group'>
     <%= f.label :住所 %>
     <%= f.text_filed :address %>
    </div>

    <div class = 'form-group'>
     <%= f.label :電話番号 %>
     <%= f.text_filed :phone_number %>
    </div>

    <div class = 'form-group'>
     <%= f.label :メールアドレス %>
     <%= f.text_filed :email %>
    </div>

    <div class = 'form-group'>
     <%= f.radio_button :is_deleted, false %>
     <%= f.label :is_deleted, "有効" %>
     <%= f.radio_button :is_deleted, false %>
     <%= f.label :is_deleted, "退会" %>
    </div>
  </div>
  <div class = 'row'>
   <%= f.submit "変更を保存", edit_admin_customer_path, class:'btn btn-success'%>
  </div>
</div>